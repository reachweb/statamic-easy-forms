{
    "name": "reachweb/statamic-easy-forms",
    "autoload": {
        "psr-4": {
            "Reach\\StatamicEasyForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reach\\StatamicEasyForms\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^4.1",
        "laravel/pint": "^1.25"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Easy Forms",
            "description": "Statamic Easy Forms addon"
        },
        "laravel": {
            "providers": [
                "Reach\\StatamicEasyForms\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
